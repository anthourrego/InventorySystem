{
    "name": "codeigniter4/appstarter",
    "type": "project",
    "description": "CodeIgniter4 starter app",
    "homepage": "https://codeigniter.com",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "almasaeed2010/adminlte": "^3.2",
        "bpampuch/pdfmake": "^0.2.4",
        "chillerlan/php-qrcode": "^4.3",
        "ckeditor/ckeditor": "^4.24",
        "codeigniter4/framework": "^4.5.1",
        "codeigniter4/translations": "^4.4",
        "components/jquery": "3.5.1",
        "datatables.net/datatables.net": "^2.0",
        "datatables.net/datatables.net-bs4": "^2.0",
        "datatables.net/datatables.net-buttons": "^3.0",
        "datatables.net/datatables.net-buttons-bs4": "^3.0",
        "datatables.net/datatables.net-scroller": "^2.4",
        "datatables.net/datatables.net-select": "^2.0",
        "fortawesome/font-awesome": "^6.1",
        "hermawan/codeigniter4-datatables": "^0.5.4",
        "moment/moment": "^2.29",
        "nelexa/zip": "^4.0",
        "phpmailer/phpmailer": "^6.7",
        "robinherbots/jquery.inputmask": "^5.0",
        "select2/select2": "^4.0",
        "setasign/fpdi": "^2.3",
        "stuk/jszip": "^3.0",
        "tecnickcom/tcpdf": "^6.7",
        "ttskch/select2-bootstrap4-theme": "^1.5",
        "twbs/bootstrap": "4.6.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.1",
        "tatter/patches": "^2.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "suggest": {
        "ext-fileinfo": "Improves mime type detection for files"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Config\\": "app/Config"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "support": {
        "forum": "https://forum.codeigniter.com/",
        "source": "https://github.com/codeigniter4/CodeIgniter4",
        "slack": "https://codeigniterchat.slack.com"
    },
    "prefer-stable": true
}

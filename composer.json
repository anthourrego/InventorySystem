{
    "name": "codeigniter4/appstarter",
    "description": "CodeIgniter4 starter app",
    "license": "MIT",
    "type": "project",
    "homepage": "https://codeigniter.com",
    "support": {
        "forum": "https://forum.codeigniter.com/",
        "source": "https://github.com/codeigniter4/CodeIgniter4",
        "slack": "https://codeigniterchat.slack.com"
    },
    "require": {
        "php": "^8.1",
        "almasaeed2010/adminlte": "^3.2.0",
        "bpampuch/pdfmake": "^0.2.16",
        "chillerlan/php-qrcode": "^5.0.3",
        "codeigniter4/framework": "^4.5.6",
        "codeigniter4/translations": "^4.5.4",
        "components/jquery": "^3.7.1",
        "datatables.net/datatables.net": "^2.1.8",
        "datatables.net/datatables.net-bs4": "^2.1.8",
        "datatables.net/datatables.net-buttons": "^3.2.0",
        "datatables.net/datatables.net-buttons-bs4": "^3.2.0",
        "datatables.net/datatables.net-scroller": "^2.4.3",
        "datatables.net/datatables.net-select": "^2.1.0",
        "fortawesome/font-awesome": "^6.7.2",
        "hermawan/codeigniter4-datatables": "^0.7.2",
        "moment/moment": "^2.30.1",
        "nelexa/zip": "^4.0.2",
        "phpmailer/phpmailer": "^6.9.3",
        "phpoffice/phpspreadsheet": "^2.3.5",
        "robinherbots/jquery.inputmask": "^5.0.9",
        "select2/select2": "^4.0.13",
        "setasign/fpdi": "^2.6.2",
        "stuk/jszip": "^3.0.1",
        "tecnickcom/tcpdf": "^6.8.0",
        "tempusdominus/bootstrap-4": "^5.1.2",
        "tinymce/tinymce": "^7.6.0",
        "ttskch/select2-bootstrap4-theme": "^1.5.2",
        "twbs/bootstrap": "^4.6.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24.1",
        "mikey179/vfsstream": "^1.6.12",
        "phpunit/phpunit": "^10.5.40"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Config\\": "app/Config/"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true,
            "robloach/component-installer": false
        }
    },
    "suggest": {
        "ext-fileinfo": "Improves mime type detection for files"
    },
    "scripts": {
        "test": "phpunit"
    },
    "prefer-stable": true
}

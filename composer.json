{
    "name": "codeigniter4/appstarter",
    "type": "project",
    "description": "CodeIgniter4 starter app",
    "homepage": "https://codeigniter.com",
    "license": "MIT",
    "require": {
        "php": "^7.3 || ^8.0",
        "codeigniter4/framework": "^4",
        "almasaeed2010/adminlte": "^3.2",
        "twbs/bootstrap": "4.6.1",
        "components/jquery": "3.5.1",
        "fortawesome/font-awesome": "^6.1",
        "datatables.net/datatables.net": "dev-master",
        "datatables.net/datatables.net-bs4": "dev-master",
        "datatables.net/datatables.net-buttons": "dev-master",
        "datatables.net/datatables.net-buttons-bs4": "dev-master",
        "datatables.net/datatables.net-scroller": "dev-master",
        "datatables.net/datatables.net-select": "dev-master",
        "stuk/jszip": "^3.0",
        "bpampuch/pdfmake": "^0.2.4",
        "hermawan/codeigniter4-datatables": "^0.5.4",
        "moment/moment": "^2.29",
        "select2/select2": "^4.0",
        "ttskch/select2-bootstrap4-theme": "^1.5",
        "robinherbots/jquery.inputmask": "^5.0",
        "tecnickcom/tcpdf": "^6.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.1"
    },
    "suggest": {
        "ext-fileinfo": "Improves mime type detection for files"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Config\\": "app/Config"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "support": {
        "forum": "http://forum.codeigniter.com/",
        "source": "https://github.com/codeigniter4/CodeIgniter4",
        "slack": "https://codeigniterchat.slack.com"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    }
}
